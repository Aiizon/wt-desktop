<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:module="clr-namespace:wt_desktop.app.Module"
        d:DataContext="{d:DesignInstance Type=module:AccountingModule}"
        x:DataType="module:AccountingModule"
        x:Class="wt_desktop.app.Module.AccountingModule"
>
    <Grid Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" MinWidth="100" />
            <ColumnDefinition Width="5" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <StackPanel Grid.Column="0">
            <TextBlock Text="WIP" />
        </StackPanel>

        <ContentControl x:Name="PageContent" Grid.RowSpan="3" Grid.Column="2" />
    </Grid>
</UserControl>
