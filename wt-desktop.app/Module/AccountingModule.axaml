<controls:BaseModule xmlns="https://github.com/avaloniaui"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                     xmlns:module="clr-namespace:wt_desktop.app.Module"
                     xmlns:controls="clr-namespace:wt_desktop.app.Controls"
                     d:DataContext="{d:DesignInstance Type=module:AccountingModule}"
                     x:DataType="module:AccountingModule"
                     x:Class="wt_desktop.app.Module.AccountingModule"
>
    <controls:BaseModule.NavContent>
        <StackPanel>
            <controls:NavigationButton Icon="Account" Label="Clients"   Command="{Binding NavigateToCustomerCommand}" />
            <controls:NavigationButton Icon="File"    Label="Locations" Command="{Binding NavigateToRentalCommand}"   />
        </StackPanel>
    </controls:BaseModule.NavContent>
</controls:BaseModule>
